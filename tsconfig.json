{
  "compilerOptions": {
    "target": "es2020",
    "jsx": "react",
    "allowJs": true,
    "checkJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@placekey/placekey": ["src"],
      "@placekey/placekey/test": ["test"],
      "test": ["test"]
    }
  },
  "exclude":[
    "node_modules",
    "**/dist/**/*"
  ]
}
